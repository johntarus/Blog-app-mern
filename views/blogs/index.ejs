<!DOCTYPE html>
<html lang="en">
<%- include("../partials/head.ejs")%>

    <body>
        <%- include("../partials/nav.ejs") %>
            <div class="blogs content">
                <h2>All Blogs</h2>
                <% data.map(blog=> { %>
                    <a class="single" href="/blogs/<%= blog._id %>">
                        <h3>
                            <%= blog.title %>
                        </h3>
                        <p>
                            <%= blog.snippet %>
                        </p>
                    </a>

                    <% }) %>
            </div>
            <%- include("../partials/footer.ejs")%>
    </body>

</html>